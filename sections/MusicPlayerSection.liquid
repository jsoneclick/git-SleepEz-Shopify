    <div class="music__block">
      {% for block in section.blocks %}
        <audio src={{ block.settings.audio_url }}" preload="auto"></audio>
        {% endfor %}
    </div>
    <script>
        const player = document.querySelector(".playerWrapper");
        let playerButtons = player.querySelectorAll(".audioPlayerButton");
        let audioBlock = player.querySelectorAll(".player_audio_element");
        const musicContainer = document.querySelector(".music__block");
        let audios = musicContainer.getElementsByTagName("audio");

        let buttonSVG = player.querySelectorAll(".button_svg");

        function toggleMusic(index) {
    for (let i = 0; i < audios.length; i++) {
        if (i === index) {
            if (audios[i].paused) {
                audios[i].currentTime = 0;
                audios[i].play();
                audios[i].volume = 0.3;
                playerButtons[i].classList.add("playing");
                playerButtons[i].innerHTML = `
                    <svg class="button_svg" width="309" height="392" viewBox="0 0 309 392" fill="none" xmlns="http://www.w3.org/2000/svg">
                        <rect width="112" height="392" fill="white"/>
                        <rect x="197" width="112" height="392" fill="white"/>
                    </svg>`;
                    playerButtons[i].style.paddingLeft = "0";
                    audioBlock[i].style.background = "#00000060";
            } else {
                audios[i].pause();
                playerButtons[i].classList.remove("playing");
                playerButtons[i].innerHTML = `
                    <svg class="button_svg" xmlns="http://www.w3.org/2000/svg" width="161" height="186" viewBox="0 0 161 186" fill="none">
                        <path d="M160.557 92.6363L0.177344 185.231L0.177352 0.0412909L160.557 92.6363Z" fill="white"/>
                    </svg>`; 
                    playerButtons[i].style.paddingLeft = "5px";
                    audioBlock[i].style.background = "none";
            }
        } else {
            audios[i].pause();
            playerButtons[i].classList.remove("playing");
            playerButtons[i].innerHTML = `
                <svg xmlns="http://www.w3.org/2000/svg" width="161" height="186" viewBox="0 0 161 186" fill="none">
                    <path d="M160.557 92.6363L0.177344 185.231L0.177352 0.0412909L160.557 92.6363Z" fill="white"/>
                </svg>`;
                playerButtons[i].style.paddingLeft = "none";
                audioBlock[i].style.background = "none";
        }
    }
}
    </script>

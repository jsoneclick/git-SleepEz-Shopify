<style>
        @import url('https://fonts.googleapis.com/css2?family=Inter:wght@100;200;300;400;500;600;700;800;900&display=swap');
        .audioPlayerWrapper {
            height: 100vh;
            width: 100vw;
            position: relative;
            background: {{ section.settings.backgroundColor }};
            box-sizing: border-box;
            overflow: hidden;
            padding: 2rem;
            font-family: 'Inter', sans-serif;
        }
        .backgroundImage {
            position: absolute;
            height: 100%;
            right: 2rem;
            top: 2rem;
            opacity: 0.2;
        }
        .backgroundImage img{
          height: 100% !important;
          width: auto !important;
          
        }
        .product_image__aside, .product_content__aside {
            height: 100%;
            width: 50%;
            box-sizing: border-box;
            position: relative;
            display: flex;
            justify-content: center;
            align-items: center;
        }
        .product_image__aside{
            float: left;
            padding-top: 20vh;
            
        }
        .product_content__aside{
            float: right;
        }
        .product_content__wrapper {
            color: #fff;
            width: 400px;
        }
        .product_content__wrapper h1 {
            font-size: clamp(3rem, -1.75rem + 12vw, 5rem);
            font-weight: 700;
            margin: 0;
            padding: 0;
        }
        .product_content__wrapper p {
            font-size: clamp(0.6rem, 0.075rem + 2vw, 0.9rem);
            margin: 0;
            padding: 0;
            font-weight: 400px;
            
        }
        .player_audio_element {
            height: auto;
            width: 100%;
            display: flex;
            justify-content: space-between;
            align-items: center;
            margin-top: 16px;
            padding: 8px 16px;
            border-radius: 16px;
        }
        .player_audio_element p {
            float: left;
            width: calc(100% - 70px);
            font-weight: 200px;
            font-size: clamp(0.75rem, 0.25rem + 1.6vw, 1.15rem);
            margin: 0;
            padding: 0;
        }
        .audioPlayerButton {
            height: 48px;
            aspect-ratio: 1 / 1;
            background: linear-gradient(180deg, #FFF9BE 0%, #BD80ED 100%);
            border-radius: 50%;
            display: flex;
            justify-content: center;
            align-items: center;
            float: right;
            cursor: pointer;
            padding-left: 5px;
            box-sizing: border-box;
        }
        .audioPlayerButton svg {
            height: 40%;
            width: 40%;
            
        }
        .playerWrapper {
            margin-top: 32px;
        }

        .lamp_image__wrapper {
            width: 65%;
            background: ;
            position: relative;
            animation: lampFloating 6s infinite;
        }
        .lamp_image__wrapper svg{
            width: 100%;
        }
        .lamp_image__wrapper svg:nth-child(1), .lamp_image__wrapper svg:nth-child(2), .lamp_image__wrapper svg:nth-child(3), .lamp_image__wrapper svg:nth-child(5) {
            position: absolute;
            width: 117%;
            left: 50%;
            transform: translate(-50%, 0%);
        }
        .lamp_image__wrapper svg:nth-child(1) {
            top: 10%;
            width: 108%;
            z-index: 4;
        }
        .lamp_image__wrapper svg:nth-child(2) {
            top: 22%;
            width: 113%;
          z-index: 4;
        }
        .lamp_image__wrapper svg:nth-child(3) {
            top: 34%;
          z-index: 4;
        }
        .lamp_image__wrapper img:nth-child(4) {
          position: relative;
            z-index: 2;
        }

        @keyframes lampFloating {
            0%, 100% {margin-top: -50px;}
            50% {margin-top: 50px;}
        }

        .lamp_image__wrapper svg:nth-child(5) {
            bottom: -150px;
            animation: lampShadowFloating 6s infinite;
        }
        @keyframes lampShadowFloating {
            0%, 100% {bottom: -180px; transform: translate(-50%, 0%) scale(.7);}
            50% {bottom: -120px; transform: translate(-50%, 0%) scale(1);};
        }
        .lamp_image__wrapper img:nth-child(6) {
            position: absolute;
            width: 50%;
            height: auto;
            border-radius: 0.6rem;
            transform: rotateZ(-19deg);
            left: 0;
            top: -33%;
            z-index: 4;
        }
        .lamp_image__wrapper svg:nth-child(7) {
            width: 200px;
            position: absolute;
            top: -85%;
            animation: lampFloating 3s infinite;
            right: -45%;
          z-index: 1;
        }

        @media screen and (max-width: 1000px) {
            .product_image__aside {
                order: 2;
                float: bottom;
                width: 100%;
                bottom: 0;
                padding-top: 0;
                height: 40%;
            }
            .product_content__aside {
                order: 1;
                float: top;
                width: 100%;
                height: 50%;
                padding: 50px;
                top: 0;
                z-index: 999;
            }
          .audioPlayerWrapper {
            height: 80vh;
          }
          
            .audioPlayerButton {
                height: 35px;
            }
            .product_content__wrapper h1, .product_content__wrapper p {
                text-align: center;
            }
            .playerWrapper {
                margin-top: 32px;
            }
            .lamp_image__wrapper img:nth-child(6) {
                width: 120px;
                top: -24%;
            }
            .backgroundImage {
                transform: rotateY(180deg);
            }
            .lamp_image__wrapper svg:nth-child(7) {
                width: 30vw;
            }
          .audioPlayerWrapper {
            padding: 1rem;
          }
        }
        @media screen and (max-width: 700px) {
          .lamp_image__wrapper svg:nth-child(7) {
              top: -150%;
          }
          .audioPlayerWrapper {
            padding: 0;
          }
          .lamp_image__wrapper {
            width: 50%;
            max-width: 500px;
          }
          
        }
  @media screen and (min-width: 1400px){
    .lamp_image__wrapper {
      width: 50%;
    }
  }

    </style>
